{"\u5916\u6587\u540d": "fermented bean curd", "\u522b\u00a0\u00a0\u00a0\u00a0\u79f0": "\u8c46\u8150\u4e73\uff0c\u732b\u4e73\uff0c\u9709\u8c46\u8150", "\u4e2d\u6587\u540d": "\u8150\u4e73", "\u4e3b\u8981\u539f\u6599": "\u9ec4\u8c46\u3001\u7c73\u9152\u3001\u98df\u76d0\u3001\u7802\u7cd6", "\u526f\u4f5c\u7528": "\u9ad8\u8840\u538b\u3001\u5fc3\u8840\u7ba1\u75c5\u3001\u75db\u98ce\u3001\u80be\u75c5\u60a3\u8005\u53ca\u6d88\u5316\u9053\u6e83\u75a1\u60a3\u8005\u98df\u7528\uff0c\u4f1a\u52a0\u91cd\u75c5\u60c5", "\u4e3b\u8981\u8425\u517b\u6210\u5206": "\u86cb\u767d\u8d28\uff0c\u9499\u8d28", "\u662f\u5426\u542b\u9632\u8150\u5242": "\u5426", "\u9002\u5b9c\u4eba\u7fa4": "\u4e00\u822c\u4eba\u7fa4\u5747\u53ef\u98df\u7528", "\u4e3b\u8981\u98df\u7528\u529f\u6548": "\u5f00\u80c3\u6d88\u98df\u8c03\u4e2d"}