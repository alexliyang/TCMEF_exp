{"\u7c7b\u522b": "\u533b\u5b66\u8457\u4f5c", "\u8bed\u79cd": "\u4e2d\u6587", "\u4f5c\u8005": "\u9648\u5ff5\u7956", "\u540d\u79f0": "\u300a\u533b\u5b66\u4ece\u4f17\u5f55\u300b", "\u7b80\u4ecb": "\u300a\u533b\u5b66\u4ece\u4f17\u5f55\u300b\u662f\u4ee5\u8bba\u6cbb\u5185\u79d1\u6742\u75c5\u4e3a\u4e3b\u7684\u533b\u4e66\u3002\u8be5\u4e66\u5185\u5bb9\u4e30\u5bcc\uff0c\u8be6\u5ba1\u8109\u8bc1\uff0c\u6df1\u7a76\u75c5\u673a\u3002\u9648\u62b5\u8bba\u533b\u591a\u672c\u533b\u5b66\u7ecf\u5178\u4e4b\u65e8\uff0c\u53c8\u80fd\u878d\u4f1a\u8d2f\u901a\uff0c\u77e5\u5e38\u8fbe\u53d8\uff1b\u6cbb\u75c7\u5584\u64b7\u524d\u4ee3\u540d\u533b\u4e4b\u957f\u548c\u6c11\u95f4\u7ecf\u9a8c\uff0c\u4e14\u9009\u5176\u7cbe\u534e\uff0c\u52a0\u4ee5\u53d1\u6325\u3002\u5982\u5bf9\u75e2\u8bc1\u3001\u6dcb\u8bc1\u3001\u75f0\u996e\u3001\u54b3\u55fd\u6cc4\u6cfb\u7b49\u75c5\u8bc1\u7684\u8bba\u6cbb\uff0c\u4f53\u73b0\u9648\u6c0f\u8fa9\u8bc1\u9009\u65b9\u7684\u7279\u70b9\u3002"}