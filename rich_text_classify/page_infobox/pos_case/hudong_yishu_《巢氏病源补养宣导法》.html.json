{"\u8bed\u79cd": "\u4e2d\u6587", "\u7b80\u4ecb": "\u6c14\u529f\u5bfc\u5f15\u4e13\u8457\u3002\u6e05\u00b7\u5ed6\u5e73\u6839\u636e\u968b\u4ee3\u5de2\u5143\u65b9\u7b49\u300a\u8bf8\u75c5\u6e90\u5019\u8bba\u300b\u6458\u7f16\u800c\u6210\u3002\u300a\u8bf8\u75c5\u6e90\u5019\u8bba\u300b\u5728\u6240\u8bba\u7684\u7edd\u5927\u90e8\u5206\u8bc1\u5019\u4e4b\u540e\uff0c\u5747\u9644\u6709\u76f8\u5e94\u7684\u6c14\u529f\u5bfc\u5f15\u6cbb\u7597\u65b9\u6cd5\uff0c\u5171\u8ba1260\u4f59\u6761\uff0c\u53ef\u8bf4\u662f\u968b\u4ee5\u524d\u533b\u7597\u6c14\u529f\u5bfc\u5f15\u6cd5\u7684\u4e00\u6b21\u603b\u7ed3\u3002", "\u88c5\u5e27": "\u5e73\u88c5", "\u4f5c\u8005": "\u5ed6\u5e73", "\u540d\u79f0": "\u300a\u5de2\u6c0f\u75c5\u6e90\u8865\u517b\u5ba3\u5bfc\u6cd5\u300b", "\u5176\u4ed6": "\u4ee5\u6240\u6cbb\u75c5\u8bc1\u8303\u56f4\u6765\u8bf4\uff0c\u5305\u62ec\u4e86\u5185\u3001\u5916\u3001\u5987\u3001\u4f24\u7b49\u5404\u79d1\u75c5\u8bc1\u3002\u5ed6\u5e73\u5c06\u4e0a\u8ff0\u6563\u89c1\u7684\u529f\u6cd5\u6458\u5f55\u6c47\u7f16\u6210\u4e66\uff0c\u6536\u5165\u5176\u300a\u516d\u8bd1\u9986\u4e1b\u4e66\u300b\u4e4b\u4e2d\u3002", "\u7c7b\u522b": "\u533b\u4e66"}