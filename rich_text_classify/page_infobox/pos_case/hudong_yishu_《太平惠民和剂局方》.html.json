{"\u8bed\u79cd": "\u4e2d\u6587", "\u7b80\u4ecb": "\u5c06\u6210\u836f\u65b9\u5242\u5206\u4e3a\u8bf8\u98ce\u3001\u4f24\u5bd2\u3001\u5987\u4eba\u8bf8\u75be\u53ca\u5c0f\u513f\u8bf8\u75be\u517114\u95e8\uff0c788\u65b9\u3002\u5747\u7cfb\u6536\u5f55\u6c11\u95f4\u5e38\u7528\u7684\u6709\u6548\u4e2d\u836f\u65b9\u5242\uff0c\u8bb0\u8ff0\u4e86\u5176\u4e3b\u6cbb\u3001\u914d\u4f0d\u53ca\u5177\u4f53\u4fee\u5236\u6cd5\u3002\u5176\u4e2d\u6709\u8bb8\u591a\u540d\u65b9\uff0c\u5982\u725b\u9ec4\u6e05\u5fc3\u4e38\u3001\u56db\u7269\u6c64\u7b49\u3002\u662f\u4e00\u90e8\u6d41\u4f20\u8f83\u5e7f\u3001\u5f71\u54cd\u8f83\u5927\u7684\u4e34\u5e8a\u65b9\u4e66\u3002", "\u88c5\u5e27": "\u5e73\u88c5", "\u522b\u540d": "\u300a\u548c\u5242\u5c40\u65b9\u300b", "\u540d\u79f0": "\u300a\u592a\u5e73\u60e0\u6c11\u548c\u5242\u5c40\u65b9\u300b", "\u5176\u4ed6": "\u6709\u7684\u520a\u672c\u5728\u4e66\u672b\u9644\u6709\u9648\u5e08\u6587\u7b49\u64b0\u300a\u56fe\u7ecf\u672c\u8349\u836f\u6027\u603b\u8bba\u300b\uff08\u4e3a\u672c\u8349\u63d0\u8981\u6027\u8d28\u7684\u8457\u4f5c\uff09\u548c\u8bb8\u6d2a\u64b0\u300a\u7528\u836f\u603b\u8bba\u6307\u5357\u300b\uff08\u4e3a\u836f\u7269\u603b\u8bba\u6027\u8d28\u7684\u8457\u4f5c\uff09\u54043\u5377\u3002\u73b0\u5b58\u591a\u79cd\u660e\u3001\u6e05\u523b\u672c\uff0c1925\u5e74\u4e0a\u6d77\u6821\u7ecf\u5c71\u623f\u77f3\u5370\u672c\u30021949\u5e74\u540e\u6709\u6392\u5370\u672c\u3002", "\u7c7b\u522b": "\u533b\u4e66", "\u51fa\u7248\u65f6\u95f4": "\n1078\u5e74\n"}