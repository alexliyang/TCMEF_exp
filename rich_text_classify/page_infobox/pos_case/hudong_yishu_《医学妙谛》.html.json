{"\u8bed\u79cd": "\u4e2d\u6587", "\u7b80\u4ecb": "\u672c\u4e66\u8bba\u8ff0\u4e2d\u98ce\u3001\u4f24\u98ce\u3001\u4e2d\u5bd2\u3001\u6691\u75c5\u7b49\u517156\u79cd\u75c5\u8bc1\uff08\u5305\u62ec\u8033\u3001\u9f3b\u3001\u53e3\u3001\u9f7f\u75c5\uff09\u7684\u75c5\u56e0\u3001\u8bc1\u6cbb\uff0c\u80fd\u878d\u4f1a\u8bf8\u5bb6\u5b66\u8bf4\uff0c\u7740\u91cd\u4ecb\u7ecd\u4e2a\u4eba\u4e34\u5e8a\u5fc3\u5f97\u3002\u56e0\u6b64\u53ef\u4ee5\u4ece\u591a\u5904\u770b\u5230\u4f5c\u8005\u7684\u72ec\u5230\u89c1\u89e3\u3002", "\u88c5\u5e27": "\u5e73\u88c5", "\u4f5c\u8005": "\u4f55\u4e66\u7530", "\u540d\u79f0": "\u300a\u533b\u5b66\u5999\u8c1b\u300b", "\u5176\u4ed6": "\u4e66\u4e2d\u81ea\u62df\u7ecf\u9a8c\u65b9\u9887\u591a\uff0c\u5e76\u5c06\u4e00\u4e9b\u75c5\u75c7\u7684\u75c5\u56e0\u3001\u6cbb\u6cd5\u7f16\u4e3a\u6b4c\u8bc0\uff0c\u4fbf\u4e8e\u8bfb\u8005\u5b66\u4e60\u3002\u73b0\u6709\u5149\u7eea\u523b\u672c\uff0c1936\u5e74\u4e1c\u4e9a\u4e66\u5c40\u94c5\u5370\u672c\u3002\u540e\u7f16\u5165\u300a\u4e09\u4e09\u533b\u4e66\u300b\u4e2d\u3002"}