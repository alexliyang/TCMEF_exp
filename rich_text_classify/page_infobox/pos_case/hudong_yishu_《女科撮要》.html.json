{"\u8bed\u79cd": "\u4e2d\u6587", "\u7b80\u4ecb": "\u4e0a\u5377\u5217\u8ff0\u7ecf\u5019\u4e0d\u8c03\u3001\u7ecf\u6f0f\u4e0d\u6b62\u7b4915\u7c7b\u5987\u79d1\u75be\u75c5\u7684\u8bc1\u6cbb\u548c\u65b9\u836f\uff1b\u4e0b\u5377\u4ecb\u7ecd\u4fdd\u80ce\u3001\u5c0f\u4ea7\u7b4915\u7c7b\u4ea7\u79d1\u75be\u75c5\u7684\u8bc1\u6cbb\u548c\u65b9\u836f\u3002\u591a\u7cfb\u660e\u4ee5\u524d\u5987\u4ea7\u79d1\u8457\u4f5c\u4e4b\u7cbe\u8403\u8bba\u8ff0\u548c\u89c4\u8303\u65b9\u836f\uff0c\u5176\u4e2d\u4ea6\u4e0d\u4e4f\u859b\u6c0f\u7684\u72ec\u5230\u89c1\u89e3\u3002", "\u88c5\u5e27": "\u5e73\u88c5", "\u4f5c\u8005": "\u859b\u5df1", "\u540d\u79f0": "\u300a\u5973\u79d1\u64ae\u8981\u300b", "\u5176\u4ed6": "\u300a\u5973\u79d1\u64ae\u8981\u300b\u662f\u4e00\u90e8\u7406\u3001\u6cd5\u3001\u65b9\u3001\u836f\u5177\u5907\u7684\u5987\u79d1\u4e13\u8457\uff0c\u7cbe\u8f9f\u8bba\u8ff0\u4e86\u7ecf\u3001\u5e26\u3001\u80ce\u3001\u4ea7\u53ca\u5987\u79d1\u6742\u75c7\u7684\u8bc1\u6cbb\uff0c\u662f\u859b\u6c0f\u5987\u4ea7\u79d1\u4e34\u8bc1\u7ecf\u9a8c\u7684\u835f\u8403", "\u7c7b\u522b": "\u53e4\u4ee3\u533b\u4e66", "\u51fa\u7248\u65f6\u95f4": "\n1548\u5e74\n"}