{"\u5546\u7528\u540d": "\u4e94\u5b50\u884d\u5b97\u4e38", "\u6027\u72b6": "\u4e38\u5242\uff08\u5927\u871c\u4e38\uff09", "\u4e3b\u8981\u6210\u5206": "\u675e\u5b50\u3001\u83df\u4e1d\u5b50(\u7092)\u3001\u8986\u76c6\u5b50\u3001\u4e94\u5473\u5b50(\u84b8)\u3001\u8f66\u524d\u5b50(\u76d0\u7092)\u7b49", "\u751f\u4ea7\u5382\u5546": "\u5317\u4eac\u540c\u4ec1\u5802\u79d1\u6280\u53d1\u5c55\u80a1\u4efd\u6709\u9650\u516c\u53f8\u5236\u836f\u5382", "\u7528\u6cd5\u7528\u91cf": "\u53e3\u670d\u3002\u6c34\u871c\u4e38\u4e00\u6b216\u514b\uff0c\u4e00\u65e52\u6b21\u3002", "\u662f\u5426\u5904\u65b9\u836f": "\u975e\u5904\u65b9\u836f", "\u836f\u54c1\u7c7b\u522b": "\u4e2d\u836f", "\u4e2d\u6587\u540d": "\u4e94\u5b50\u884d\u5b97\u4e38", "\u9002\u5e94\u75c7": "\u8865\u80be\u76ca\u7cbe\u3002\u7528\u4e8e\u80be\u865a\u7cbe\u4e8f\u6240\u81f4\u7684\u9633\u75ff\u4e0d\u80b2\u3001\u9057\u7cbe\u65e9\u6cc4\u3001\u8170\u75db\u3001\u5c3f\u540e\u4f59\u6ca5\u3002", "\u5305\u88c5\u89c4\u683c": "\u5927\u871c\u4e38\u6bcf\u4e38\u91cd9g"}