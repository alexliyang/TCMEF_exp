{"\u8bed\u79cd": "\u4e2d\u6587", "\u7b80\u4ecb": "\u672c\u4e66\u4e3b\u8981\u4ee5\u9648\u81ea\u660e\u300a\u5987\u4eba\u5927\u5168\u826f\u65b9\u300b\uff08\u5df2\u6821\u6ce8\u672c\uff09\u4e3a\u84dd\u672c\uff0c\u5e7f\u6cdb\u6536\u96c6\u5404\u5bb6\u5b66\u8bf4\uff0c\u5bf9\u660e\u4ee5\u524d\u5987\u79d1\u5b66\u6210\u5c31\u4f5c\u4e86\u8f83\u7ec6\u7edf\u7684\u6574\u7406\u3002\u6210\u4e3a\u4ee3\u8868\u660e\u4ee3\u6c34\u5e73\u7684\u5987\u4ea7\u79d1\u4e13\u8457\u3002\u5168\u4e66\uff1a\u6cbb\u6cd5\u603b\u8bba\u3001\u8c03\u7ecf\u95e8\u3001\u6742\u8bc1\u95e8\u3001\u80ce\u524d\u95e8\u53ca\u4ea7\u540e\u95e8\u4e94\u5927\u7c7b\uff0c\u6bcf\u7c7b\u5206\u5217\u82e5\u5e72\u75c5\u75c7\uff0c\u5bf9\u4e8e\u75c5\u56e0\u3001\u8fa8\u8bc1\u53ca\u6cbb\u7597\u8bba\u8ff0\u9887\u8be6\u3002", "\u88c5\u5e27": "\u7cbe\u88c5", "\u522b\u540d": "\u300a\u5973\u79d1\u51c6\u7ef3\u300b", "\u4f5c\u8005": "\u738b\u80af\u5802", "\u540d\u79f0": "\u300a\u8bc1\u6cbb\u51c6\u7ef3\u00b7\u5973\u79d1\u300b", "\u5176\u4ed6": "\u662f\u4e00\u90e8\u8d44\u6599\u6bd4\u8f83\u4e30\u5bcc\u7684\u5987\u79d1\u8457\u4f5c\u3002\u7248\u672c\u60c5\u51b5\u53c2\u89c1\u300a\u516d\u79d1\u8bc1\u6cbb\u51c6\u7ef3\u300b\u6761\u3002\u660e\u00b7\u6e05\u53ca\u6c11\u56fd\u65f6\u671f\u6709\u5355\u884c\u672c\uff0c1949\u5e74\u540e\u6709\u5f71\u5370\u672c\u53ca\u6392\u5370\u672c\u3002", "\u7c7b\u522b": "\u5987\u79d1\u8457\u4f5c"}