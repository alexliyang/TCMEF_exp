{"\u8bed\u79cd": "\u4e2d\u6587", "\u7b80\u4ecb": "\u8be6\u7ec6\u8bba\u8ff0\u4e8636\u5589\u98ce\u7684\u540d\u79f0\u3001\u75c7\u72b6\u3001\u6cbb\u6cd5\u3001\u672b\u6cbb\u7597\u5589\u98ce\u7684\u65b9\u836f\uff0c\u4ee5\u53ca\u7259\u75b3\u3001\u5589\u95f4\u53d1\u767d\uff08\u767d\u5589\uff09\u7b49\u75c7\u3002\u5377\u4e0b\u4e3a\u201c\u5589\u98ce\u9488\u8bc0\u201d\uff0c\u5305\u62ec\u5589\u79d1\u75be\u75c5\u5e38\u7528\u7684\u5341\u56db\u7ecf\u547d\u7a74\u3001\u884c\u9488\u624b\u6cd5\u3001\u8865\u6cfb\u548c\u7981\u5fcc\u3002\u5185\u5bb9\u7ed3\u5408\u4e34\u5e8a\uff0c\u6bd4\u8f83\u5b9e\u7528", "\u88c5\u5e27": "\u5e73\u88c5", "\u4f5c\u8005": "\u90d1\u5b8f\u7eb2", "\u540d\u79f0": "\u300a\u91cd\u697c\u7389\u94a5\u300b", "\u5176\u4ed6": "\u6b64\u4e66\u5bf9\u767d\u5589\u8bca\u6cbb\u53ca\u5b9c\u5fcc\u5c24\u8be6\uff0c\u4e3b\u6cbb\u4e4b\u65b9\u4e3a\u517b\u9634\u6e05\u80ba\u6c64\u3002\u5b9e\u4e3a\u4eca\u65e5\u6297\u767d\u5589\u5408\u5242\u7684\u7956\u65b9\u3002", "\u7c7b\u522b": "\u533b\u4e66"}