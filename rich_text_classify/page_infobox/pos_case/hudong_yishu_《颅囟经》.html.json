{"\u8bed\u79cd": "\u4e2d\u6587", "\u7b80\u4ecb": "\u5185\u5bb9\u9996\u8bba\u8109\u6cd5\uff0c\u6b21\u8bba\u75c5\u6e90\u3001\u75c5\u8bc1\uff0c\u518d\u6b21\u4e3a\u60ca\u75eb\u766b\u8bc1\u6cbb\u3001\u75b3\u75e2\u8bc1\u6cbb\u53ca\u706b\u4e39\u8bc1\u6cbb\u3002\u6587\u5b57\u7b80\u7565\uff0c\u662f\u73b0\u5b58\u8f83\u65e9\u7684\u513f\u79d1\u4e13\u4e66\u3002\u73b0\u6709\u300a\u5f53\u5f52\u8349\u5802\u533b\u5b66\u4e1b\u4e66\u300b\u672c\u3002", "\u88c5\u5e27": "\u5e73\u88c5", "\u522b\u540d": "\u300a\u5e08\u5deb\u9885\u56df\u7ecf\u300b", "\u4f5c\u8005": "\u536b\u6c5b", "\u540d\u79f0": "\u300a\u9885\u56df\u7ecf\u300b", "\u5176\u4ed6": "\u4e66\u540d\u53d6\u5c0f\u513f\u521d\u751f\u65f6\u9885\u56df\u672a\u5408\u4e4b\u4e49\u3002\u53c8\u540d\u300a\u5e08\u5deb\u9885\u56df\u7ecf\u300b\u3002\u4e8c\u5377\uff08\u4e00\u4f5c\u4e09\u5377\uff09\u3002\u6258\u540d\u5468\u7a46\u738b\u65f6\u201c\u5e08\u5deb\u201d\u6240\u4f20\uff08\u4e00\u4f5c\u4e1c\u6c49\u00b7\u536b\u6c5b\u64b0\uff09\u3002\u660e\u4ee3\u4ee5\u540e\u539f\u4e66\u5df2\u4f5a\uff0c\u4eca\u4e4b\u6240\u5b58\u4e43\u8f91\u81ea\u300a\u6c38\u4e50\u5927\u5178\u300b\u7684\u300a\u56db\u5e93\u5168\u4e66\u300b\u672c\u5df2\u975e\u5168\u5e19\u3002", "\u7c7b\u522b": "\u513f\u79d1\u8457\u4f5c"}