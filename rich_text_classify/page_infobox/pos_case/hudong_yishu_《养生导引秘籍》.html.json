{"\u8bed\u79cd": "\u4e2d\u6587", "\u7b80\u4ecb": "\u672c\u4e66\u4e3a\u6211\u56fd\u53e4\u4ee3\u517b\u751f\u4fdd\u5065\u53ca\u6c14\u529f\u5bfc\u5f15\u7684\u4e1b\u4e66\u3002\u517134\u79cd\uff0c\u8ba168\u5377\u3002\u5185\u5bb9\u8ff0\u53ca\u517b\u751f\u4fdd\u5065\u3001\u5ef6\u5e74\u76ca\u5bff\u7684\u996e\u98df\u3001\u8d77\u5c45\u3001\u8c03\u6444\u548c\u60c5\u5fd7\u7684\u4fee\u517b\u53ca\u7981\u5fcc;\u6c14\u529f\u53ca\u517b\u751f\u5b66\u4e2d\u7684\u547c\u5438\u5bfc\u5f15\u529f\u6cd5\u3001\u5e38\u7528\u7684\u98df\u7597\u836f\u7269\u7b49\uff0c", "\u88c5\u5e27": "\u5e73\u88c5", "\u4f5c\u8005": "\u80e1\u6587\u7115", "\u540d\u79f0": "\u300a\u517b\u751f\u5bfc\u5f15\u79d8\u7c4d\u300b", "\u5176\u4ed6": "\u672c\u4e66\u65e2\u6709\u7406\u8bba\u6307\u5bfc\u610f\u4e49\uff0c\u4e5f\u6709\u8eab\u5fc3\u4fdd\u517b\u4e0e\u953b\u70bc\u7684\u5177\u4f53\u65b9\u6cd5\uff0c\u7279\u522b\u5bf9\u4e2d\u8001\u5e74\u4eba\u7684\u5ef6\u5e74\u76ca\u5bff\u3001\u795b\u75c5\u9632\u75c5\u3001\u4fee\u8eab\u660e\u6027\u53ca\u5987\u5973\u7684\u9a7b\u989c\u7f8e\u5bb9\u6709\u72ec\u5230\u4e4b\u5904\u3002", "\u7c7b\u522b": "\u533b\u4e66"}